package pt.ulht.es.cookbook.domain;


class Receita {
	String Titulo;
	String Problema;
	String Solucao;
	String Autor;
	String Tags;
	DateTime creationTimestamp;
}
 
 class CookbookManager;
 
 
 relation CookbookManagerHasReceitas {
	CookbookManager playsRole cookbookManager { multiplicity 1..1; }
	Receita playsRole receita { multiplicity 0..*; }	
}